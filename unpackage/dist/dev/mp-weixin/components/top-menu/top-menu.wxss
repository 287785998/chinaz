.wrap-header {
  height: 100rpx;
  background: #fefefe;
  box-sizing: border-box;
  z-index: 1003;
  border-bottom: 2rpx solid #f0f0f0;
}
.wrap-header .left {
  padding: 10rpx 0;
}
.wrap-header .left, .wrap-header .right {
  position: relative;
  float: left;
  font-size: 0;
  z-index: 10;
}
.wrap-header .logo {
  width: 236rpx;
  height: 64rpx;
  background-size: cover;
}
.wrap-header .right {
  float: right;
  display: flex;
  align-items: center;
  height: 100%;
}

